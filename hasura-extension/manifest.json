{
  "manifest_version": 2,
  "name": "Hasura Environment Differentiator",
  "version": "1.0",
  "description": "Visually differentiate Hasura environments",
  "permissions": [
    "http://localhost:8080/*",
    "http://localhost:8081/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://localhost:8080/console/*", "http://localhost:8081/console/*"],
      "js": ["inject.js"]
    }
  ],
  "web_accessible_resources": [
    "config.json"
  ]
} 